# Warframe Market API
WM_BASE_URL=https://api.warframe.market/v1
PLATFORM=pc
STRATEGY=balanced
REFRESH_INTERVAL_SECONDS=45

# Database (set to false to run in memory-only mode)
USE_DB=false
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db:5432/wth

# Platform fee (default 0 for Warframe - trades cost credits, not plat)
PLATFORM_FEE_PCT=0

# Backend
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Database credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=wth
